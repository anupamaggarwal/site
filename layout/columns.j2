{# Columns template #}

{% extends "topbar.j2" %}

{% block basecss %}
{{ super() }}
<link href="{{ media_url('css/columns.css') }}" rel="stylesheet">
{% endblock basecss %}


{% block content %}

<div class="page-header">
</div>

<div class="row">
  <div class="span10">
      {% filter markdown -%}
      {% block main %}{% endblock %}
      {%- endfilter %}
  </div>
  <div class="span4">
    {% block sidebar %}{% endblock %}
  </div>
</div>

{% endblock content %}



